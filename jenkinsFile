          stage('deploy')
         {
         steps{
deploy adapters: [tomcat8(credentialsId: 'e2b3978b-d681-4c72-93be-2110850568b3', path: '', url: 'http://13.233.139.13:8880/')], contextPath: 'deploy', onFailure: false, war: '**/*.war'         }
         }
         }
}
