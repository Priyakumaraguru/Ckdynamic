pipeline
{
    agent any
    
    tools{
    maven 'maven'
    }
    
   stages{
    stage('Build')
        {
            steps{
              sh 'mvn clean install'
                 }
         }
         stage('SonarQube analysis') {
       steps{
        withSonarQubeEnv('SonarQube') {
            sh 'mvn verify org.sonarsource.scanner.maven:sonar'
           }
       }
   }
}
}
