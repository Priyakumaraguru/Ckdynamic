@Library('shlib1')_
pipeline
{
    agent any
    tools{
    maven 'Maven'
    }

    
   stages{
    stage('Build')
        {
            steps{
              sh 'mvn clean install'
                 }
         }
         
         stage('Nexus')
    {
     steps{
          
            sh   'curl  -u admin:admin123 "http://3.15.18.214:8081/nexus/content/local/repositories/${repoid}/status" '
         
}
}
}
}
